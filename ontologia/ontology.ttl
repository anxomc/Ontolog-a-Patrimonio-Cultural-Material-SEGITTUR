    rdfs:label "Templo Islámico" ;
    rdfs:subClassOf tcm:Templo .

tcm:TemploJudío a owl:Class ;
    rdfs:label "Templo Judío" ;
    rdfs:subClassOf tcm:Templo .

tcm:TieneBienCultural a owl:ObjectProperty ;
    rdfs:label "tiene bien cultural" ;
    rdfs:domain segittur:TourismDestination ;
    rdfs:range tcm:BienCultural .

tcm:TieneHorario a owl:ObjectProperty ;
    rdfs:label "tiene horario" ;
    rdfs:domain tcm:BienCulturalInmueble ;
    rdfs:range segittur:Itinerary .

tcm:TieneItinerario a owl:ObjectProperty ;
    rdfs:label "tiene itinerario" ;
    rdfs:domain tcm:Tour ;
    rdfs:range segittur:Itinerary .

tcm:VisitaBienInmueble a owl:ObjectProperty ;
    rdfs:label "visita bien inmueble" ;
