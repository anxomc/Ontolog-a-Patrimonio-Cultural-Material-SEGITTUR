    rdfs:range tcm:BienCulturalInmueble .

tcm:VisitaDestino a owl:ObjectProperty ;
    rdfs:label "visita destino" ;
    rdfs:domain tcm:Persona ;
    rdfs:range segittur:TourismDestination .

tcm:tieneAplicacionTarifa a owl:ObjectProperty ;
    rdfs:label "tiene aplicacion tarifa" ;
    rdfs:domain tcm:Tour ;
    rdfs:range tcm:AplicacionTarifa .

tcm:tieneOGD a owl:ObjectProperty ;
    rdfs:label "tiene o g d" ;
    rdfs:domain segittur:TourismDestination ;
    rdfs:range tcm:OGD .

tcm:tieneOficina a owl:ObjectProperty ;
    rdfs:label "tiene oficina" ;
    rdfs:domain segittur:TourismDestination ;
    rdfs:range segittur:TouristInformationOffice .

tcm:tieneTarifa a owl:ObjectProperty ;
    rdfs:label "tiene tarifa" ;
    rdfs:domain tcm:BienCulturalInmueble ;
    rdfs:range tcm:Tarifa .

tcm:tieneTarjeta a owl:ObjectProperty ;
    rdfs:label "tiene tarjeta" ;
    rdfs:domain segittur:TourismDestination ;
    rdfs:range tcm:TarjetaTurística .

<https://ontologia.segittur.es/turismo/def/turismo-cultural-material/> a owl:Ontology ;
    dcterms:created "2025-11-10"^^xsd:date ;
    dcterms:creator <https://github.com/anxomc>,
        <https://w3id.org/people#AContributorURI> ;
    dcterms:description "Extensión de la ontología de SEGITTUR, caso de: Turismo Cultural Material"@es ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:title "Ontología de Turismo Cultural Material (Extensión de SEGITTUR)"@es ;
    vann:preferredNamespacePrefix "segittur",
        "tcm" ;
    vann:preferredNamespaceUri <https://ontologia.segittur.es/turismo/def/core#>,
        <https://ontologia.segittur.es/turismo/def/turismo-cultural-material/> ;
    owl:versionIRI <https://ontologia.segittur.es/turismo/def/turismo-cultural-material/0.1.0> ;
    owl:versionInfo "0.1.0" ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

tcm:InstrumentoCientíficoTécnico a owl:Class ;
    rdfs:label "Instrumento Científico Técnico" ;
    rdfs:subClassOf tcm:BienCulturalMueble .

tcm:OGD a owl:Class ;
    rdfs:label "O G D" .

tcm:ObraDeArte a owl:Class ;
    rdfs:label "Obra De Arte" ;
    rdfs:subClassOf tcm:BienCulturalMueble .

tcm:TemploCristiano a owl:Class ;
    rdfs:label "Templo Cristiano" ;
    rdfs:subClassOf tcm:Templo .

segittur:Monument a owl:Class ;
    rdfs:label "Monument" ;
    rdfs:subClassOf tcm:BienCulturalInmueble .

tcm:ActoDeCompra a owl:Class ;
    rdfs:label "Acto De Compra" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tcm:TarjetaTurística ;
            owl:onProperty tcm:TarjetaComprada ] .

tcm:AplicacionTarifa a owl:Class ;
    rdfs:label "Aplicacion Tarifa" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tcm:Tarifa ;
            owl:onProperty tcm:TarifaBase ] .

tcm:ObjetoArqueológico a owl:Class ;
    rdfs:label "Objeto Arqueológico" ;
    rdfs:subClassOf tcm:BienCulturalMueble .

tcm:ObjetoEtnográfico a owl:Class ;
    rdfs:label "Objeto Etnográfico" ;
    rdfs:subClassOf tcm:BienCulturalMueble .

tcm:Templo a owl:Class ;
    rdfs:label "Templo" ;
    rdfs:subClassOf tcm:Edificio .

segittur:Itinerary a owl:Class ;
    rdfs:label "Itinerary" .

tcm:BienCultural a owl:Class ;
    rdfs:label "Bien Cultural" .

tcm:Documento a owl:Class ;
    rdfs:label "Documento" ;
    rdfs:subClassOf tcm:BienCulturalMueble .

tcm:Tarifa a owl:Class ;
    rdfs:label "Tarifa" .

tcm:Tour a owl:Class ;
    rdfs:label "Tour" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tcm:AplicacionTarifa ;
            owl:onProperty tcm:tieneAplicacionTarifa ],
        [ a owl:Restriction ;
            owl:allValuesFrom segittur:Itinerary ;
            owl:onProperty tcm:TieneItinerario ] .

segittur:TouristInformationOffice a owl:Class ;
    rdfs:label "Tourist Information Office" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tcm:TarjetaTurística ;
            owl:onProperty tcm:OfreceTarjeta ] .

tcm:BienCulturalMueble a owl:Class ;
    rdfs:label "Bien Cultural Mueble" ;
    rdfs:subClassOf tcm:BienCultural .

tcm:Persona a owl:Class ;
    rdfs:label "Persona" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom segittur:TouristInformationOffice ;
            owl:onProperty tcm:AcudeOficina ],
        [ a owl:Restriction ;
            owl:allValuesFrom tcm:ActoDeCompra ;
            owl:onProperty tcm:Compra ],
        [ a owl:Restriction ;
            owl:allValuesFrom segittur:TourismDestination ;
            owl:onProperty tcm:VisitaDestino ],
        [ a owl:Restriction ;
            owl:allValuesFrom tcm:Tour ;
            owl:onProperty tcm:AcudeVisita ],
        [ a owl:Restriction ;
            owl:allValuesFrom tcm:BienCulturalInmueble ;
            owl:onProperty tcm:VisitaBienInmueble ] .

segittur:TourismDestination a owl:Class ;
    rdfs:label "Tourism Destination" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom segittur:TouristInformationOffice ;
            owl:onProperty tcm:tieneOficina ],
        [ a owl:Restriction ;
            owl:allValuesFrom tcm:OGD ;
            owl:onProperty tcm:tieneOGD ],
        [ a owl:Restriction ;
            owl:allValuesFrom tcm:TarjetaTurística ;
            owl:onProperty tcm:tieneTarjeta ],
        [ a owl:Restriction ;
            owl:allValuesFrom tcm:BienCultural ;
            owl:onProperty tcm:TieneBienCultural ] .

tcm:Edificio a owl:Class ;
    rdfs:label "Edificio" ;
    rdfs:subClassOf tcm:BienCulturalInmueble .

tcm:TarjetaTurística a owl:Class ;
    rdfs:label "Tarjeta Turística" .

tcm:BienCulturalInmueble a owl:Class ;
    rdfs:label "Bien Cultural Inmueble" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tcm:Tarifa ;
            owl:onProperty tcm:tieneTarifa ],
        [ a owl:Restriction ;
            owl:allValuesFrom segittur:Itinerary ;
            owl:onProperty tcm:TieneHorario ],
        tcm:BienCultural .

