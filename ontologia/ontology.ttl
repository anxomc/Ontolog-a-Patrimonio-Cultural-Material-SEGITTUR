tcm:precioPagado a owl:DatatypeProperty ;
    rdfs:label "precio pagado" .

tcm:temporada a owl:DatatypeProperty ;
    rdfs:label "temporada" .

tcm:tipoVisitante a owl:DatatypeProperty ;
    rdfs:label "tipo visitante" .

owl:versionIRI a owl:AnnotationProperty .

owl:versionInfo a owl:AnnotationProperty .

tcm:AcudeOficina a owl:ObjectProperty ;
    rdfs:label "acude oficina" ;
    rdfs:domain tcm:Persona ;
    rdfs:range segittur:TouristInformationOffice .

tcm:AcudeVisita a owl:ObjectProperty ;
    rdfs:label "acude visita" ;
    rdfs:domain tcm:Persona ;
    rdfs:range tcm:Tour .

tcm:Compra a owl:ObjectProperty ;
    rdfs:label "compra" ;
    rdfs:domain tcm:Persona ;
    rdfs:range tcm:ActoDeCompra .

tcm:OfreceTarjeta a owl:ObjectProperty ;
    rdfs:label "ofrece tarjeta" ;
    rdfs:domain segittur:TouristInformationOffice ;
    rdfs:range tcm:TarjetaTurística .

tcm:TarifaBase a owl:ObjectProperty ;
    rdfs:label "tarifa base" ;
    rdfs:domain tcm:AplicacionTarifa ;
    rdfs:range tcm:Tarifa .

tcm:TarjetaComprada a owl:ObjectProperty ;
    rdfs:label "tarjeta comprada" ;
    rdfs:domain tcm:ActoDeCompra ;
    rdfs:range tcm:TarjetaTurística .

